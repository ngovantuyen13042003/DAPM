@model DAPM.Models.TbDotLu

@{
    ViewData["Title"] = "Delete Flood Event";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Delete Flood Event</h1>

<h3>Are you sure you want to delete this event: @Model.TenDotLu?</h3>

<form asp-action="Delete" asp-controller="DOTLU" method="post">
    <input type="hidden" asp-for="IdDotLu" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
